<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
    <string name="app_name">Sunshine Watchface</string>
    <string name="message">Watch face tapped</string>
    <string name="my_digital_name">Sunshine Digital</string>

    <!-- This string is displayed on tap. It contains humidity and wind speed and direction
    values -->
    <string name="humidity_and_wind_toast">
        Humidity: <xliff:g id="humidity">%1$s</xliff:g>%%, Wind: <xliff:g id="wind">%2$s</xliff:g>
